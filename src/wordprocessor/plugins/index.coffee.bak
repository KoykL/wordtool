events= require("events").EventEmitter
class processword extends events
	constructor: ()->
		
	process: (words, argv)->
		if argv["with-index"]
			for each, i in words
				each["index"] = i
			#console.log("Now I emit")
			#@words = words
			@processed = true
		else
			@processed = false
		this.emit("end")
		#return @words
	processed: ()->
		return @processed
exports.processword = processword